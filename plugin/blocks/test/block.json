	  "text": {
		"type": "string",
		"default": ""
	  },
	  "textColor": {
		"type": "string",
		"default": "primary-700"
	  },
	  "backgroundColor": {
		"type": "string",
		"default": "primary-300"
	  }
	},
	"editorScript": "tdlsp-blocks"
