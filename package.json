{
  "name": "wp-project-boilerplate",
  "title": "WordPress Project Boilerplate",
  "slug": "lhpbp",
  "version": "0.0.1",
  "description": "A base boilerplate for Luehrsen // Heinrich WordPress projects. ",
  "scripts": {
    "webpack": "webpack",
    "watch": "webpack --watch",
    "start": "npx wp-env start",
    "stop": "npx wp-env stop",
    "env:clean": "npx wp-env clean all",
    "update:schemas": "node bin/updateThemeJsonSchema.js",
    "update:versions": "node bin/updatePluginVersion.js && node bin/updateThemeVersion.js",
    "prestart": "npm install && composer install && npm run update:schemas && npm run webpack",
    "lint:php": "composer run lint",
    "dev": "npm run start && npm run watch"
  },
  "author": "Luehrsen // Heinrich",
  "authorUrl": "https://www.luehrsen-heinrich.de",
  "license": "GPLv2",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@luehrsenheinrich/postcss-wp-global-styles": "^0.2.1",
    "@wordpress/babel-plugin-import-jsx-pragma": "^3.1.2",
    "@wordpress/babel-preset-default": "^6.6.1",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.4.1",
    "@wordpress/env": "^4.3.1",
    "archiver": "^5.3.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.4",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "normalise": "^10.0.0",
    "normalize.css": "^8.0.1",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-remove-empty-scripts": "^0.7.3"
  }
}
