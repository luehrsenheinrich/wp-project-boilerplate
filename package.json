{
  "name": "wp-project-boilerplate",
  "title": "WordPress Project Boilerplate",
  "slug": "lhpbp",
  "version": "0.0.1",
  "description": "A base boilerplate for Luehrsen // Heinrich WordPress projects. ",
  "scripts": {
    "webpack": "webpack",
    "watch": "webpack --watch",
    "start": "npx wp-env start",
    "stop": "npx wp-env stop",
    "clean": "npx wp-env clean all",
    "schemas": "node bin/updateThemeJsonSchema.js",
    "prestart": "npm install && composer install && npm run schemas && npm run webpack",
    "lint": "composer run lint"
  },
  "author": "Luehrsen // Heinrich",
  "authorUrl": "https://www.luehrsen-heinrich.de",
  "license": "GPLv2",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@wordpress/env": "^4.3.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.4",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "normalise": "^10.0.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.8",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-remove-empty-scripts": "^0.7.3"
  }
}
